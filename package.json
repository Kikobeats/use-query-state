{
  "name": "@kikobeats/use-query-state",
  "description": "React hook to set/retrieve state from URL query string",
  "homepage": "https://nicedoc.io/Kikobeats/use-query-state",
  "version": "1.1.1",
  "main": "index.js",
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://kikobeats.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kikobeats/use-query-state.git"
  },
  "bugs": {
    "url": "https://github.com/Kikobeats/use-query-state/issues"
  },
  "keywords": [
    "gatsby",
    "hook",
    "next",
    "nextjs",
    "query",
    "ssr",
    "state"
  ],
  "dependencies": {
    "dequal": "~2.0.2",
    "flat": "~5.0.2",
    "qss": "~2.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "conventional-github-releaser": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "git-dirty": "latest",
    "nano-staged": "latest",
    "npm-check-updates": "latest",
    "prettier-standard": "latest",
    "react": "latest",
    "simple-git-hooks": "latest",
    "standard": "latest",
    "standard-version": "latest"
  },
  "engines": {
    "node": ">= 12"
  },
  "files": [
    "create.js",
    "gatsby.js",
    "index.js"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "contributors": "(git-authors-cli && finepack && git add package.json && git commit -m 'build: contributors' --no-verify) || true",
    "lint": "standard",
    "postrelease": "npm run release:tags && npm run release:github && npm publish",
    "prerelease": "git-dirty && npm run update:check && npm test && npm run contributors",
    "pretest": "npm run lint",
    "release": "git-authors-cli finepack && git add package.json && standard-version -a",
    "release:github": "conventional-github-releaser -p angular",
    "release:tags": "git push --follow-tags origin master",
    "test": "exit 0",
    "update": "ncu -u",
    "update:check": "ncu -- --error-level 2"
  },
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "nano-staged": {
    "*.js": [
      "prettier-standard"
    ],
    "package.json": [
      "finepack"
    ]
  },
  "peerDependencies": {
    "react": "^17",
    "react-dom": "^17"
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit",
    "pre-commit": "npx nano-staged"
  },
  "type": "module"
}
